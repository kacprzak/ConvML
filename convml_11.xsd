<?xml version="1.0" encoding="utf-8" ?>
<!--Created with Liquid XML Studio - FREE Community Edition 7.0.2.746 (http://www.liquid-technologies.com)-->
<xs:schema xmlns="http://www.entertech.com.pl/bcml" targetNamespace="http://www.entertech.com.pl/bcml" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation>
Metajęzyk opisu elementów przenośników taśmowych. Służy do zbierania informacji oraz do zarządzania danymi dotyczącymi przenośników.

Author: Marcin Kacprzak
Company: EnterTECH
Id: $Id$
</xs:documentation>
  </xs:annotation>
  <!-- **************************************************************************************
    *                                        CONVML                                         *
    ************************************************************************************** -->
  <xs:element name="ConvML" type="convMLType" />
  <!-- * -->
  <xs:element name="CONVML" substitutionGroup="ConvML" type="convMLType" />
  <!-- * -->
  <xs:complexType name="convMLType">
    <xs:annotation>
      <xs:documentation>Główny element dokumentu</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Meta" nillable="true">
        <xs:annotation>
          <xs:documentation>Metadane dokumentu</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:all minOccurs="0">
            <xs:element name="Generator" type="xs:string">
              <xs:annotation>
                <xs:documentation>Program który utworzył dokument</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Title" type="xs:string">
              <xs:annotation>
                <xs:documentation>Opcjonalny tytuł dokumentu</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="xs:string">
              <xs:annotation>
                <xs:documentation>Opcjonalny opis dokumentu</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Creator" type="xs:token">
              <xs:annotation>
                <xs:documentation>Użytkownik który utworzył dokument</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="CreationDate" type="xs:dateTime">
              <xs:annotation>
                <xs:documentation>Data utworzenia dokumentu</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ModifiedBy" type="xs:token">
              <xs:annotation>
                <xs:documentation>Użytkownik który ostatnio modyfikował dokuemnt</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ModifiedDate" type="xs:dateTime">
              <xs:annotation>
                <xs:documentation>Data ostatniej modyfikacji</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Language">
              <xs:annotation>
                <xs:documentation>Kod języka</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:token">
                    <xs:attribute name="link" type="xs:anyURI">
                      <xs:annotation>
                        <xs:documentation>Link do pliku z tłumaczeniami</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="UnitSystem">
              <xs:annotation>
                <xs:documentation>System jednostek</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:token">
                    <xs:attribute name="link" type="xs:anyURI">
                      <xs:annotation>
                        <xs:documentation>Link do pliku z jednostkami</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Types">
        <xs:annotation>
          <xs:documentation>Typy elemetów skadowych przenośnika</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="MaterialType">
              <xs:complexType>
                <xs:complexContent mixed="false">
                  <xs:extension base="materialType">
                    <xs:attributeGroup ref="typeAttrGroup" />
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="BeltSegmentType">
              <xs:complexType>
                <xs:attributeGroup ref="typeAttrGroup" />
                <xs:attributeGroup ref="beltSegmentAttrGroup" />
              </xs:complexType>
            </xs:element>
            <xs:element name="BeltSpliceType">
              <xs:complexType>
                <xs:complexContent mixed="false">
                  <xs:extension base="beltSpliceType">
                    <xs:attributeGroup ref="typeAttrGroup" />
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="IdlerType">
              <xs:complexType>
                <xs:complexContent mixed="false">
                  <xs:extension base="idlerType">
                    <xs:attributeGroup ref="typeAttrGroup" />
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
        <xs:key name="materialKey">
          <xs:selector xpath="MaterialType" />
          <xs:field xpath="@typeId" />
        </xs:key>
        <xs:key name="beltSegmentKey">
          <xs:selector xpath="BeltSegmentType" />
          <xs:field xpath="@typeId" />
        </xs:key>
        <xs:key name="beltSpliceKey">
          <xs:selector xpath="BeltSpliceType" />
          <xs:field xpath="@typeId" />
        </xs:key>
        <xs:key name="idlerKey">
          <xs:selector xpath="IdlerType" />
          <xs:field xpath="@typeId" />
        </xs:key>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="unbounded" ref="BeltConveyor" />
    </xs:sequence>
    <xs:attribute fixed="1.2" name="version" type="xs:token" />
    <!--xs:attributeGroup ref="dataBaseGroup" /-->
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        CONDITIONS                                     *
    ************************************************************************************** -->
  <xs:element name="Conditions" type="conditionsType" />
  <!-- * -->
  <xs:element name="CONDITIONS" substitutionGroup="Conditions" type="conditionsType" />
  <!-- * -->
  <xs:complexType name="conditionsType">
    <xs:annotation>
      <xs:documentation>Warunki pracy przenośnika</xs:documentation>
    </xs:annotation>
    <!-- xs:attributeGroup ref="instanceAttrGroup" / -->
    <xs:attribute name="ambientTemperature" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>Temperatura otoczenia przenośnika - T [°C]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="workConditions">
      <xs:annotation>
        <xs:documentation>Warunki pracy przenośnika: 1 - bardzo dobre, 2 - dobre, 3 - przeciętne, 4 - ciężkie</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="1" />
          <xs:enumeration value="2" />
          <xs:enumeration value="3" />
          <xs:enumeration value="4" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="beltWorkConditions">
      <xs:annotation>
        <xs:documentation>Warunki eksploatacji taśmy: 1 - bardzo dobre, 2 - dobre, 3 - przeciętne, 4 - ciężkie</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="1" />
          <xs:enumeration value="2" />
          <xs:enumeration value="3" />
          <xs:enumeration value="4" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="beltSideRunning">
      <xs:annotation>
        <xs:documentation>Zbieganie boczne taśmy: 1 - brak, 2 - małe, 3 - średnie, 4 - duże</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="1" />
          <xs:enumeration value="2" />
          <xs:enumeration value="3" />
          <xs:enumeration value="4" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="amountWorkingHours" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Ilość godzin pracy przenośnika w ciągu roku [h]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        MATERIAL                                       *
    ************************************************************************************** -->
  <xs:element name="Material" type="materialType" />
  <!-- * -->
  <xs:element name="MATERIAL" substitutionGroup="Material" type="materialType" />
  <!-- * -->
  <xs:complexType name="materialType">
    <xs:annotation>
      <xs:documentation>Materiał transportowany</xs:documentation>
    </xs:annotation>
    <xs:attribute name="name" type="xs:token">
      <xs:annotation>
        <xs:documentation>Nazwa materiału</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="density" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Gęstość nasypowa materiału transportowanego - γ [kg/m³]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="surchargeAngle" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Kąt usypu materiału na taśmie - ρ [°]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="reposeAngle" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Kąt tarcia wewnętrznego [°]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lumpSizeMaximum" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Maksymalny wymiar brył [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lumpPercTopSize" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Procentowy udział brył [%]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="materialBeltFriction" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Współczynni tarcia urobek-taśma</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="temperature" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Temperatura materiału [°C]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        BELT CONVEYOR                                  *
    ************************************************************************************** -->
  <xs:element name="BeltConveyor" type="beltConveyorType" />
  <!-- * -->
  <xs:element name="BELT_CONVEYOR" substitutionGroup="BeltConveyor" type="beltConveyorType" />
  <!-- * -->
  <xs:complexType name="beltConveyorType">
    <xs:annotation>
      <xs:documentation>Przenośnik taśmowy</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="Conditions" />
      <xs:element ref="Belt" />
      <xs:element ref="Tail" />
      <xs:element ref="Route" />
      <xs:element ref="Head" />
      <xs:element ref="ConvElectricalEquipment" />
    </xs:sequence>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <xs:attribute name="beltSpeed" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Prędkość taśmy przenośnika - v [m/s]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="designCapacity" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Wydajność nominalna przenośnika - Q [t/h]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="localization" type="xs:token">
      <xs:annotation>
        <xs:documentation>Wyrobisko</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mine" type="xs:token">
      <xs:annotation>
        <xs:documentation>Kopalnia</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mineArea" type="xs:token">
      <xs:annotation>
        <xs:documentation>Rejon</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="convDivision" type="xs:token">
      <xs:annotation>
        <xs:documentation>Oddział taśmowy</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mineDivision" type="xs:string">
      <xs:annotation>
        <xs:documentation>Obsługiwane oddziały wydobywcze</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="maxCapacity" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Wydajność maksymalna</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        BELT                                           *
    ************************************************************************************** -->
  <xs:element name="Belt" type="beltType" />
  <!-- * -->
  <xs:element name="BELT" substitutionGroup="Belt" type="beltType" />
  <!-- * -->
  <xs:complexType name="beltType">
    <xs:annotation>
      <xs:documentation>Taśma</xs:documentation>
    </xs:annotation>
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <!-- odwrócenie kolejności na życzenie klienta z ZG Rudna -->
      <xs:element ref="BeltSplice" />
      <xs:element ref="BeltSegment" />
    </xs:sequence>
    <!-- xs:attributeGroup ref="instanceAttrGroup" / -->
    <xs:attribute name="width" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Szerokość taśmy - B [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        ROUTE                                          *
    ************************************************************************************** -->
  <xs:element name="Route" type="routeType" />
  <!-- * -->
  <xs:element name="ROUTE" substitutionGroup="Route" type="routeType" />
  <!-- * -->
  <xs:complexType name="routeType">
    <xs:annotation>
      <xs:documentation>Trasa przenośnika</xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="RouteSection" />
    </xs:choice>
    <xs:attributeGroup ref="instanceAttrGroup" />
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        HEAD                                           *
    ************************************************************************************** -->
  <xs:element name="Head" type="headTailType">
    <xs:annotation>
      <xs:documentation>Stacja czołowa przenośnika - wysięgnik</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="HEAD" substitutionGroup="Head" type="headTailType" />
  <!-- **************************************************************************************
    *                                        TAIL                                           *
    ************************************************************************************** -->
  <xs:element name="Tail" type="headTailType">
    <xs:annotation>
      <xs:documentation>Stacja zwrotna przenośnika - zwrotnia</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TAIL" substitutionGroup="Tail" type="headTailType" />
  <!-- **************************************************************************************
    *                                        BELT SEGMENT                                   *
    ************************************************************************************** -->
  <xs:element name="BeltSegment" type="beltSegmentType">
    <xs:keyref name="beltSegmentRef" refer="beltSegmentKey">
      <xs:selector xpath="." />
      <xs:field xpath="@type" />
    </xs:keyref>
  </xs:element>
  <!-- * -->
  <xs:element name="BELT_SEGMENT" substitutionGroup="BeltSegment" type="beltSegmentType" />
  <!-- * -->
  <xs:attributeGroup name="beltSegmentAttrGroup">
    <xs:attribute name="width" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Szerokość odcinka taśmy [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="strength" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Nominalna wytrzymałość taśmy [kN/m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mass" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Masa taśmy [kg/m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="thickness" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Grubość taśmy [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="topCoverThickness" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Grubość okładki górnej (nośnej) [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="bottomCoverThickness" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Grubość okładki dolnej (bieżnej) [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="carcassThickness" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Grubość rdzenia taśmy [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="carcassType" type="xs:token">
      <xs:annotation>
        <xs:documentation>Typ rdzenia taśmy (tkaninowa, z linkami stalowymi)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="carcassCode" type="xs:token">
      <xs:annotation>
        <xs:documentation>Współczynnik rodzaju materiału rdzenia</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="elasticityModulus" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Moduł sprężystości taśmy - E [kN/m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pliesNumber" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Liczba przekładek lub linek</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="cableDiameter" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Średnica linki [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="coverDescription" type="xs:token">
      <xs:annotation>
        <xs:documentation>Oznaczenie okładek</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="coversDensity" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Gęstość mieszanki okładkowej [kg/mm*m²]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- * -->
  <xs:complexType name="beltSegmentType">
    <xs:annotation>
      <xs:documentation>Odcinek taśmy pomiędzy połączeniami</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <xs:attributeGroup ref="beltSegmentAttrGroup" />
    <xs:attribute name="length" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Długość odcinka [m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="certificate" type="xs:string">
      <xs:annotation>
        <xs:documentation>Numer atestu</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        BELT SPLICE                                    *
    ************************************************************************************** -->
  <xs:element name="BeltSplice" type="beltSpliceType">
    <xs:keyref name="beltSpliceRef" refer="beltSpliceKey">
      <xs:selector xpath="." />
      <xs:field xpath="@type" />
    </xs:keyref>
  </xs:element>
  <!-- * -->
  <xs:element name="BELT_SPLICING" substitutionGroup="BeltSplice" type="beltSpliceType" />
  <xs:attributeGroup name="beltSpliceAttrGroup">
    <xs:attribute name="spliceStrength" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Wytrzymałość połączenia [%]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spliceType" type="xs:string">
      <xs:annotation>
        <xs:documentation>Rodzaj zlącza np: wulkanizowane, mechaniczne, klejone, inne.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- * -->
  <xs:complexType name="beltSpliceType">
    <xs:annotation>
      <xs:documentation>Połączenie odcinków taśmy</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <xs:attributeGroup ref="beltSpliceAttrGroup" />
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        CARRY                                          *
    ************************************************************************************** -->
  <xs:element name="Carry" type="carryReturnType">
    <xs:annotation>
      <xs:documentation>Górna trasa</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CARRY" substitutionGroup="Carry" type="carryReturnType" />
  <!-- **************************************************************************************
    *                                        RETURN                                         *
    ************************************************************************************** -->
  <xs:element name="Return" type="carryReturnType">
    <xs:annotation>
      <xs:documentation>Dolna trasa</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="RETURN" substitutionGroup="Return" type="carryReturnType" />
  <!-- **************************************************************************************
    *                                        IDLER SET                                      *
    ************************************************************************************** -->
  <xs:element name="IdlerSet" type="idlerSetType" />
  <!-- * -->
  <xs:element name="IDLER_SET" substitutionGroup="IdlerSet" type="idlerSetType" />
  <!-- * -->
  <xs:complexType name="idlerSetType">
    <xs:annotation>
      <xs:documentation>Zestaw krążnikowy</xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="5">
      <xs:element ref="Idler" />
    </xs:choice>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <!--xs:attribute name="type" /-->
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        IDLER                                          *
    ************************************************************************************** -->
  <xs:element name="Idler" type="idlerType" />
  <!-- * -->
  <xs:element name="IDLER" substitutionGroup="Idler" type="idlerType" />
  <!-- * -->
  <xs:complexType name="idlerType">
    <xs:annotation>
      <xs:documentation>Pojedynczy krążnik</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="2" ref="Bearing" />
    </xs:sequence>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <xs:attribute name="idlerType" type="xs:token">
      <xs:annotation>
        <xs:documentation>Typ krążnika: 1 - z pierścieniami, 2 - gładki</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="diameter" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Średnica krążnika [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="length" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Długość płaszcza krążnika [mm]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="troughingAngle" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Kąt nachylenia krążników bocznych - β [°]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rotatingPartsMass" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Masa części obrotowych krążnika [kg]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup ref="pulleyIdlerGroup" />
  </xs:complexType>
  <!-- **************************************************************************************
    *                                        CHUTE                                          *
    ************************************************************************************** -->
  <xs:element name="Chute" type="chuteType" />
  <!-- * -->
  <xs:element name="CHUTE" substitutionGroup="Chute" type="chuteType" />
  <!-- * -->
  <xs:complexType name="chuteType">
