  <xs:complexType name="routeSegmentType">
    <xs:annotation>
      <xs:documentation>Segment trasy</xs:documentation>
    </xs:annotation>
    <xs:sequence minOccurs="0">
      <xs:element ref="Carry" />
      <xs:element ref="Return" />
    </xs:sequence>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <!--xs:attributeGroup ref="lengthAngleGroup" /-->
    <xs:attribute name="length" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Długość elementu [m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <!--xs:attribute name="number" type="xs:nonNegativeInteger" /-->
  </xs:complexType>
  <!-- **************************************************************************************
       *                                        UNLOAD                                         *
       ************************************************************************************** -->
  <xs:element name="Unload" type="unloadType" />
  <!-- * -->
  <xs:element name="UNLOAD" substitutionGroup="Unload" type="unloadType" />
  <!-- * -->
  <xs:complexType name="unloadType">
    <xs:annotation>
      <xs:documentation>Rozładunek</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="instanceAttrGroup" />
  </xs:complexType>
  <!-- **************************************************************************************
       *                                        COUNTERWEIGHT                                  *
       ************************************************************************************** -->
  <xs:element name="Counterweight" type="counterweightType" />
  <!-- * -->
  <xs:element name="COUNTERWEIGHT" substitutionGroup="Counterweight" type="counterweightType" />
  <!-- * -->
  <xs:complexType name="counterweightType">
    <xs:annotation>
      <xs:documentation>Obciążnik</xs:documentation>
    </xs:annotation>
    <xs:attribute name="discsNumber" type="xs:nonNegativeInteger">
      <xs:annotation>
        <xs:documentation>Oliczba krążków</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mass" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Masa obciążnika</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup ref="instanceAttrGroup" />
  </xs:complexType>
  <!-- ********************************************** -->
  <xs:complexType name="carryReturnType">
    <xs:annotation>
      <xs:documentation>Typ zawartości dla elementów CARRY i RETURN</xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded">
      <xs:element minOccurs="0" ref="Pulley" />
      <xs:element minOccurs="0" ref="TTDrive" />
      <xs:element minOccurs="0" ref="Chute" />
      <xs:element minOccurs="0" ref="IdlerSet" />
      <xs:element minOccurs="0" ref="IdlerSetSpecial" />
      <xs:element minOccurs="0" ref="SlipPlate" />
      <xs:element minOccurs="0" ref="ElectricalEquipment" />
      <xs:element minOccurs="0" ref="AdditionalEquipment" />
      <xs:element minOccurs="0" ref="Unload" />
    </xs:choice>
    <!--xs:attributeGroup ref="instanceAttrGroup" /-->
  </xs:complexType>
  <!-- **************************************************** -->
  <xs:complexType name="headTailType">
    <xs:annotation>
      <xs:documentation>Typ zawartości dla elementów HEAD i TAIL</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element ref="Pulley" />
      <xs:element ref="Carry" />
      <xs:element ref="Return" />
    </xs:all>
    <xs:attributeGroup ref="instanceAttrGroup" />
    <xs:attributeGroup ref="lengthAngleGroup" />
  </xs:complexType>
  <!-- **************************************************** -->
  <xs:simpleType name="nonNegativeDecimalType">
    <xs:annotation>
      <xs:documentation>Typ xs:decimal o wartościach nieujemnych</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:minInclusive value="0" />
    </xs:restriction>
  </xs:simpleType>
  <!-- ATTRIBUTE GROUPS *********************************** -->
  <!--xs:attributeGroup name="dataBaseGroup">
      <xs:annotation>
      <xs:documentation>Atrybuty powiązane z obsługą relacyjnych baz danych</xs:documentation>
      </xs:annotation>
      <xs:attribute name="id" type="xs:long" />
      </xs:attributeGroup-->
  <!-- **************************************************** -->
  <xs:attributeGroup name="instanceAttrGroup">
    <xs:annotation>
      <xs:documentation>Atrybuty występujące we wszystkich elementach</xs:documentation>
    </xs:annotation>
    <xs:attribute name="id" type="xs:ID">
      <xs:annotation>
        <xs:documentation>Numer identyfikacyjny</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="description" type="xs:string">
      <xs:annotation>
        <xs:documentation>Dowolny opis elementu</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="productionDate" type="xs:date">
      <xs:annotation>
        <xs:documentation>Data produkcji</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="type" type="xs:token">
      <xs:annotation>
        <xs:documentation>Wskazuje typ elementu zdefiniowany w ConvML/Types/</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="typeAttrGroup">
    <xs:attribute name="manufacturer" type="xs:token">
      <xs:annotation>
        <xs:documentation>Producent elementu</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="typeId" type="xs:token" use="required">
      <xs:annotation>
        <xs:documentation>Kod identyfikacyjny typu</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="typeName" type="xs:token">
      <xs:annotation>
        <xs:documentation>Nazwa handlowa</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="typeDescription" type="xs:string" />
  </xs:attributeGroup>
  <!-- **************************************************** -->
  <xs:attributeGroup name="lengthAngleGroup">
    <xs:annotation>
      <xs:documentation>Długość i kąt nachylenia odcinak lub segentu trasy</xs:documentation>
    </xs:annotation>
    <xs:attribute name="length" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Długość elementu [m]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="angle">
      <xs:annotation>
        <xs:documentation>Kąt nachylenia elementu [°]</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="-90" />
          <xs:maxInclusive value="90" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- **************************************************** -->
  <xs:attributeGroup name="takeUpTensionGroup">
    <xs:attribute name="takeUpTension" type="xs:decimal" />
  </xs:attributeGroup>
  <!-- **************************************************** -->
  <xs:attributeGroup name="pulleyIdlerGroup">
    <xs:attribute name="mass" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Masa [kg]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="inertiaMoment" type="nonNegativeDecimalType">
      <xs:annotation>
        <xs:documentation>Moment bezwładności - I [kgm²]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
</xs:schema>