@CONFERENCE{Williams2010,
  author = {K. C. Williams, and M. G. Jones, I. Jabs, and S. Tan,},
  title = {Transient Phenomena in Dense Phase Pneumatic Transport of a Flyash
	Powder},
  booktitle = {BulkEurope 2010},
  year = {2010},
  organization = {TUNRA Bulk Solids, The Universty of Newcastle, AUSTRALIA
	
	Leibniz Universität Hannover, GERMANY},
  abstract = {Currently, the pressure prediction of fine powder dense phase pneumatic
	
	conveying systems are dominated by steady state analysis. Despite
	ongoing research, these
	
	steady state based approaches have only produced, at best, marginal
	increases in pressure
	
	drop predictive capability. This is due to the nature of these dense
	flows as they exhibit
	
	transient and pulsatile type flow behaviours, which clearly, are not
	steady state. In addition,
	
	minimal research has been conducted on the behaviour of the particles
	or particulate volumes
	
	within these dense flows. This paper reports on a comprehensive study
	undertaken at
	
	Newcastle to investigate the nature of non-steady conveying which
	utilised an electrical
	
	capacitance tomography (ECT) system. In particular, some significant
	findings with regard
	
