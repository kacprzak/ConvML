@ELECTRONIC{Rozentals,
  author = {Rozentals, J.J.},
  title = {The Design of Troughing Idlers},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon1}},
  note = {Beltcon, 1981},
  url = {http://www.saimh.co.za/beltcon/beltcon1/paper13.html},
  abstract = {The design of troughing idler components, to SABS 1313-1980 and complying
