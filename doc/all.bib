@ELECTRONIC{Feltes,
  author = {Feltes, M. J.},
  title = {Advancements in Mechanical Belt Fastener Technology},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon9}},
  organization = {FLEXIBLE STEEL LACING COMPANY},
  note = {Beltcon, 1997},
  url = {http://www.saimh.co.za/beltcon/beltcon9/paper96.html},
  abstract = {The improvements made to the designs of conveyor belt fastening systems
	have been directly related to changes in both conveyor belt design
	and construction, as well as the need to meet new requirements of
	industry. This paper is intended to briefly review past conveyor
	belt fastener designs and applications, and also to review the improvements
	made to these products over the years 
	
	
	The importance that mechanical belt fasteners has played to the global
