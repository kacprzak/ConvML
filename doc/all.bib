@ELECTRONIC{Babich,
  author = {Babich, K.},
  title = {Environmental Law and Conveyors},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon10}},
  organization = {Development Consultants of SA (Pty) Ltd., RSA},
  note = {Beltcon, 1999},
  url = {http://www.saimh.co.za/beltcon/beltcon10/paper1014.html},
  abstract = {Conveyors have wide applications in many spheres of our everyday life,
	but none more so than in mining, civils and construction. Apart from
	being vital cogs in the transportation of a wide variety of materials,
	conveyors are generally regarded as efficient and versatile.
	
	
	Whilst mining, civils and construction have negative impacts on the
	environment, conveyors as such, are not regarded as major sources
	of environmental pollution if properly operated and maintained. In
	fact, conveyors may make a positive contribution to the environment
	in that they can substitute motorised transport systems.
	
	
	The environment is increasingly receiving the attention of our global
	society and in some countries the penalties are extremely high. South
	Africa has joined this trend with the promulgation of a considerable
	number of Environmental Acts and Bills, most of which are going to
	have a direct impact on the responsibilities and liabilities of directors,
	other officials and employees as well as their employers.
	
	
	The Institute of Directors King Report on Corporate Governance released
	in November 1994 for the first time crystallised guidelines of so-called
	"best practice". The fact that some of these guidelines are being
	incorporated into legislation is a clear indication of the way in
	which things are moving. It is now a requirement to comply with Corporate
	Governance for a listing on the Johannesburg Stock Exchange.
	
	
	In my presentation I will attempt to bring the Environment, Corporate
	Governance, important legislation as well as the Constitution and
	conveyors under the spotlight and will highlight the main areas that
	will need attention.},
  owner = {Darek},
  timestamp = {2011.03.19}
}

