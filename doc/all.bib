@ELECTRONIC{Gilbertb,
  author = {Gilbert, P. R.},
  title = {Management \& Control of high elongating belt In long centered conveyors},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon12}},
  organization = {Nepean Conveyors International Pty. Ltd},
  note = {Beltcon, 2003},
  url = {http://www.saimh.co.za/beltcon/beltcon12/paper1202.htm},
  abstract = {During my career in sales & design of conveyor systems, especially
	the most recent years, I have noticed the lack of grasp many decision-makers
	have of the basic dynamic characteristics of a conveyor and the implications
	of labeling it "just another conveyor".
	
	
	I have been at this caper for twenty-six years and to date, have never
	found two exactly the same yet.
	
	
	One of the most fundamental issues, more so with Solid Woven carcass
	belt when it is selected for a conveyor is its comparison to a large
	elastic band.
	
	
	Many hear and read conveyor belt specifications but still not seem
	to grasp, in practical terms, the reality of how the conveyor will
	behave as a dynamic system and the then required conveyor system
	control process that is required to be put in place to manage the
	dynamics of the conveyor.
	
	
	This paper is not a technical brief or a matrix of math resulting
	in a theoretical conclusion. I purely wish to share in layman's terms
	some pretty staggering examples of the results & realities of selecting
	a particularly "stretchy" belt for use in a longer centered conveyor
	system and few suggestions at the ways, means & methods of controlling
	it.},
  owner = {Darek},
  timestamp = {2011.03.19}
}

@ELECTRONIC{Gilbertc,
  author = {Gilbert, P. R.},
  title = {KD01 Conveyor - Kyuga Coal, Anglo-American, Hunter Valley, Australia},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon13}},
  organization = {Nepean Conveyors International Ply, Ltd},
  note = {Beltcon, 2005},
  url = {http://www.saimh.co.za/beltcon/beltcon13/KD01%20Conveyor%20WEBPAGE/KD01%20Conveyor.htm},
  abstract = {This paper discusses an Australian project, the Kyuga Three-Seam "Hump-Back"
	Decline Conveyor Project. The engineering departments of the author's
	company and the Anglo American owned Kyuga Coal were responsible
	for the design.
	
	
	
	Kyuga Coal held a large investment in tunnelling and pre-existing
	trunk conveyor infrastructure in the Dartbrook Seam. Poor geological
	conditions led to underutilisation of the Dartbrook Seam, resulting
	in the possible shutdown of the operation.
	
	
	
	The Coal Seam above Dartbrook (Kyuga) and the coal seam above Kyuga
	(Piercefield) were the next prospects to be mined. Each of these
	would require, in normal procedural circumstances, considerable expense
	and time to drive and install a further set of trunk conveyor drive
	age & infrastructure prior to mining activities resuming for the
	Kyuga and then the Piercefield prospect.
	
	
	
	As with all new coal mine prospects, the capital infrastructure is
	weighed off against the return on investment and, methods were sought
	to reduce the capital investment for the Kyuga and Piercefield opportunities.
	The likelihood of successfully mining them would depend on reduction
	of capital & greatest utilization of existing infrastructure.
	
	
	
	Kyuga project management proposed a "Three-Seam Hump-Back Conveyor"
	(See Sketch # 1) which would carry the mined coal from the new Kyuga
	Seam up to the Piercefield Seam and then down to the Dartbrook Seam
	via a storage bunker. This proposal would enable the existing enormously
	valuable "Hunter Tunnel" drivage, conveyors & infrastructure to be
	used for the extraction of the entire Kyuga seam.
	
	
	
	The author's company proposed a conveyor system incorporating Variable
	Voltage Variable Frequency (VVVF) drive technology that would maximise
	the utilisation of the existing infrastructure. The features of the
	system allowed for, following mining the Kyuga seam, the shortening
	of the "Hump Back" conveyor from tail end towards delivery point
	thus positioning a new "Load" point in the Piercefield seam further
	above the Kyuga seam such that the same conveyor and existing infrastructure
	& drivage could be utilized along with the Dartbrook Hunter Tunnel
	infrastructure.},
  owner = {Darek},
  timestamp = {2011.03.19}
}

