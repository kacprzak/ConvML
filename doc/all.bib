@CONFERENCE{Pang2010,
  author = {Yusong Pang and Gabriel Lodewijks},
  title = {Three Decades Monitoring of Belt Conveyor Systems},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {For more than three decades, Condition Monitoring (CM) has been used
