}

@ELECTRONIC{Surtees,
  author = {Surtees, A. J.},
  title = {Longitudinal Stresses Occuring in Long Conveyor Belts During Starting
	\& Stopping},
  language = {english},
  howpublished = {\url { http://www.saimh.co.za/beltcon/beltcon3}},
  organization = {SURTEES \& SON, MINING \& INDUSTRIAL DIVISION},
  note = {Beltcon, 1985},
  url = {http://www.saimh.co.za/beltcon/beltcon3/paper33.html},
  abstract = {High transient stresses during starting and stopping a conveyor can
	cause tremendous harm to a conveyor system, resulting in belt and
	structure damage and belt splice and pulley failures.
	
	
	In some cases, it is difficult to measure these stresses and hence
	their presence may go undetected, resulting in low availability and
	high running costs of the conveyor system.
	
	
	A number of in-depth papers have been published on this subject, with
	various mathematical models being proposed.
	
	
	This paper concerns a case study on a conveyor belt suspected of having
	high transient stresses. The measurements taken are analysed with
	simple correlation to the mathematical models, and solutions to reduce
	or eliminate the stresses are discussed. A second case study is also
	briefly discussed.},
  owner = {DAREK},
  timestamp = {2011.03.16}
}

@ELECTRONIC{Surteesd,
  author = {Surtees, A. J.},
  title = {Conveyor System Commissioning, Maintenance and Failure Analysis Using
	Black Box Techniques},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon8}},
  organization = {Conveyor Watch (Pty) Ltd},
  note = {Beltcon, 1995},
  url = {http://www.saimh.co.za/beltcon/beltcon8/paper824.html},
  abstract = {This paper discusses the history and developments of sophisticated
	monitoring equipment used to measure the operating parameters of
	conveyor systems.
	
	
	With the radical reduction in price of computer hardware and software
	during the last decade and an associated increase in memory capacity,
	the concept of continuous conveyor monitor is now a reality. With
	this tool, the various problems normally encountered in large conveyor
	installations can now be avoided at an early stage.
	
	
	The four main areas of application are: 
	
	
	•Design verification 
	
	
	•Hand-over certification 
	
	
	•Preventative maintenance and increase of system availability 
	
	
	•Failure analysis 
	
	
	The paper introduces the techniques used and gives examples where
