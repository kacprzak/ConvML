@CONFERENCE{Freeman2010,
  author = {T.C. Freeman, and R.E. Freeman, and B. Armstrong,},
  title = {The characterisation of bulk material properties –
	
	the need for a multivariate approach},
  booktitle = {BulkEurope 2010},
  year = {2010},
  organization = {Freeman Technology, Boulters Farm Centre, Castlemorton Common,
	
	Welland, Malvern, Worcestershire, WR13 6LE.},
  abstract = {Powders and bulk materials cannot be viewed as invariant entities.
	Their physical
	
	properties can differ significantly from supplier to supplier; they
	may have natural
	
	variation through change in compositional proportionality (such as
	mineral ores);
	
	environmental variation such as humidity could alter their cohesion/adhesion;
	and of
	
	course actually processing/transferring the material may change its
	particle size
	
	distribution, morphology or surface texture. All of these may significantly
	impact their
	
	process behaviour/response.
	
	In the light of this reasoning it is surprising that many people still
	attempt to generate a
	
	single measure of ‘behaviour’.
	
	This presentation will cover the measurement of dynamic, bulk and
	shear properties of
	
	powders and how a comprehensive knowledge of this wide range of powder
	properties can
	
	help with the design and troubleshooting of particular processes by
	using specific
	
	examples.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{Brosh2010,
  author = {Tamir Brosh, and Haim Kalmanm and Avi Levy,},
  title = {Fragments spawning and interaction for DEM breakage
	
	simulation},
  booktitle = {BulkEurope 2010},
  year = {2010},
  organization = {Department of Mechanical Engineering
	
	Ben-Gurion University of the Negev
	
	P.O.Box 653, Beer-Sheva 84105, Israel},
  abstract = {Particles breakage occurs in many industrial applications. During
	the last decade many works have
	
	been devoted to modeling and simulating such processes. New and innovative
	procedure of empirical
	
	comminution functions for Discrete Element Method (DEM) simulations
	(Kalman et al. [1]) posed the
	
	question how to introduce the fragments of the broken particle back
	into the computational domain.
	
	Daughter particles (Fragments) spawning and interaction imposes several
	problems during DEM
	
	simulation. Some of the main problems are: seeding (allocation) daughter
	particles and their initial
	
	conditions i.e. fragments locations, velocities and physical properties.
	This work focuses on the
	
	daughter particles seeding and the interaction between them. Fragments
	spawning and interaction
	
	algorithm for particle breakage during DEM simulation was developed.
	In the proposed model, sister
	
	particles can overlap without increasing the energy of the system
	in the simulation. Particle-particle
	
	and particle-wall interaction are calculated using the standard DEM
	calculations. Daughter particles
	
	interactions were calculated using a temporary contact radius model.
	The model was used to predict
	
	particle comminution in jet milling and particle attrition during
	pneumatic conveying with great
	
	successes.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{TamirBrosh,
  author = {Tamir Brosh, and Tal Tsory, and Nir Ben-Jacob, and Avi Levy,},
  title = {Thermal DEM-CFD modeling and simulation},
  booktitle = {BulkEurope 2010},
  organization = {Pearlstone Centre for Aeronautical Engineering Studies
	
	Department of Mechanical Engineering
	
	Ben-Gurion University of the Negev
	
	P.O.Box 653, Beer-Sheva 84105, Israel},
  abstract = {Heat transfer in particulate systems is encountered in many industrial
	applications, yet is poorly
	
	understood even in simple cases due to the stress and contact heterogeneities
	inherent to these systems.
	
	In the present study, heat transfer models for particle-particle interaction
	and particle-wall interaction
	
	were developed and integrated into our in house DEM software. Heat
	transfer simulations for
	
	predicting the effective thermal conductivity (ETC) of particulate
	beds under compression were
	
	conducted in order to validate these models with experimental published
	data. The simulations include
	
	heat transfer by conduction via the pores of the particulate bed and
	thermal resistance in the contact
	
	areas between the particles.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

