@CONFERENCE{Boschert2010,
  author = {Dr. Stefan Boschert, and Dr.-Ing. Torsten Hellmuth,},
  title = {Simulation in bulk material handling},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {Software-based simulations are used to analyze systems which are too
	complex for simple theoretical
	
	or formula based examinations. Many dynamic systems can only be modelled
	and examined by
	
	simulations. Among other things, SIEMENS AG plans and delivers turnkey
	plants for a number of
	
	raw material processing industries. This frequently involves examining
	the flows of bulk materials.
	
	The “Simulation Tool for Conveying Systems” STOCS has been developed
	for this purpose.
	
	Optimizing the planning, design and implementation of conveyor systems
	is a continual challenge for
	
	system planners. STOCS supports these processes, and enables them
	to be designed more efficiently.
	
	In particular, this dynamic simulation tool is used for dimensioning
	of the process system components,
	
	operation principles and the directly related design of the material
	handling equipment. The use of
	
	STOCS is explained and its capabilities are illustrated by taking
	the example of a complex storage
	
	location for a steel plant, which is used to store supplies for a
	coke plant, a blast furnace and a sinter
	
	plant.},
