}

@ELECTRONIC{Steynberg,
  author = {Steynberg, P. J. and Braasch, W. H.},
  title = {Overview and Current State of the Art in Belt Conveying in South
	Africa},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon2}},
  note = {Beltcon, 1983},
  url = {http://www.saimh.co.za/beltcon/beltcon2/paper21.html},
  abstract = {A review is given of belt conveyor technology at present applied in
	South Africa. 
	
	
	The paper illustrates how the latest specifications and design aids
	can be used to ensure that properly chosen designs are arrived at.
	A look is taken at mechanical and electrical components in use. Motors,
	gearboxes and other major items are reviewed. Reference is made to
	new developments in the field of safety mechanisms and control systems.
	Refurbishing of belting and "mothballing" of conveyor installation
	are also discussed. 
	
	
	Special reference is made to Iscor, one of the largest users of conveyor
	belting in the country. The firm's progress in the field of belt
	conveyors is discussed, as well as the possible effects of these
	developments on national and international technology.},
  owner = {DAREK},
  timestamp = {2011.03.16}
}

@ELECTRONIC{Stilin,
  author = {Stilin, John J.},
  title = {Backstop Selection Procedure for Multiple and Single Drive Conveyors},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon3}},
  organization = {The Falk Corporation Subsidiary of Sundstrand Corporation},
  note = {Beltcon, 1985},
  url = {http://www.saimh.co.za/beltcon/beltcon3/paper316.html},
  abstract = {The safety of inclined conveyor systems relies heavily on the proper
	selection of backstops. This paper provides an analysis for such
	start-up, systems illustrating the function of the backstop(s) during
	steady operation and stopping phases.
	
	
	A detailed procedure is given for the selection of the appropriate
	sized backstop in the design stages based on the specifications of
	the conveyor system. Emphasis is on backstop size selection for load
	sharing in a multiple drive system for single belt conveyors. The
	reliability of the selection procedure is confirmed by field test
	load measurements on a multiple drive conveyor system.},
  owner = {DAREK},
  timestamp = {2011.03.16}
}

@ELECTRONIC{Surteesa,
  author = {Surtees, A.},
  title = {Further Case Studies in Transient Stresses in Belt Conveyors},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon4}},
  note = {Beltcon, 1987},
  url = {http://www.saimh.co.za/beltcon/beltcon4/paper411.html},
  abstract = {High oscillating tensions may be excited in conveyor belts due to
	unsatisfactory starting and stopping methods. These transient tensions
	may be of such large magnitudes that severe damage may be incurred
	by the belting and conveyor system.
	
	
	In this paper some tests, performed on various conveyors in the South
	African coal handling industry, are discussed to highlight typical
	problems encountered.
	
	
	Ways of reducing the magnitudes of these transient tensions and preventing
	their occurrence at the design stage are suggested, to improve the
	life and availability of the conveyor.
	
	
	This paper may be considered as a continuation of the paper presented
	at Beltcon III (Ref 1). That paper should be referred to for a more
	practical introduction to the topic of belt oscillations.},
  owner = {DAREK},
  timestamp = {2011.03.18}
}

@ELECTRONIC{Surteesc,
  author = {Surtees, A.},
  title = {Incline Conveyors: The Importance of T2 Take-up Tension to Stop Runback
	- Case Study},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon8}},
  organization = {Surtees Power Transmission},
  note = {Beltcon, 1995},
  url = {http://www.saimh.co.za/beltcon/beltcon8/paper82.html},
  abstract = {This paper puts forward 4 different actual incidents of conveyor runback
	and their case studies. These incidents occurred on four different
	mines over the last 3 to 4 years.
	
	
	By presenting this paper, we hope to benefit the users of conveyors
	in order that they prevent similar incidents from happening by informing
	them of the possible causes. The paper will also be beneficial to
	conveyor designers manufacturers and equipment suppliers.},
  owner = {Darek},
  timestamp = {2011.03.18}
}

@ELECTRONIC{Surteesf,
  author = {Surtees, A.},
  title = {Verification of Conveyor Designs, Post Instalation},
  language = {english},
  howpublished = {\url {http://www.saimh.co.za/beltcon/beltcon10}},
  organization = {Conveyor Watch Pty, Ltd., RSA},
  note = {Beltcon, 1999},
  url = {http://www.saimh.co.za/beltcon/beltcon10/paper1013.html},
  abstract = {During the last 15 years, Conveyor Watch has been involved in over
	300 post installation conveyor investigations. These investigations
	were done either to verify the designs or to check the incorrectness
	of these conveyor systems. Our task was to try to correct, or by
	definition, to set right by altering or adjusting the conveyor.
	
	
	This paper will discuss the source and type of problems encountered
	in these investigations, with the aim being to assist future designers
	and users by highlighting areas that need special attention. It will
	be shown that the problems are mainly not due to poor design, but
	rather due to poor design specifications, equipment not meeting good
	design specifications, or incompatibility of equipment used.},
  owner = {Darek},
  timestamp = {2011.03.19}
}

