@CONFERENCE{Roberts2010,
  author = {Alan W. Roberts, and Tobias Krull, and Stephen J. Wiche,},
  title = {The Influence of Surcharge Pressure on Mass-Flow Hopper Design},
  booktitle = {BulKEurope 2010},
  year = {2010},
  abstract = {Current procedures for mass-flow hopper design are based on the radial
	stress theory developed by
	
	Jenike and involve the determination of the critical outlet dimensions
	for flow to be initiated. These
	
	procedures ignore the surcharge loads on the hopper and involve a
	study of the passive or arched
	
	stress field that exists under flow conditions. An alternative approach
	as applied to hopper wall load
	
	analysis is to employ the horizontal slice method. The arched stress
	field and horizontal slice methods
	
	of analysis are compared. The influence of surcharge loads in the
	determination of hopper geometry is
	
	investigated. The paper is illustrated by an industrial case study
	example involving the rapid loading
	
	of iron ore into a wedge-shaped mass-flow hopper. In this case the
	influence of the impact pressures
	
	due to loading are examined.},
  keywords = {mass-flow, surcharge pressures, cohesive arching},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{Roberts2010a,
  author = {Alan W. Roberts, and Tobias Krull, and Stephen J. Wiche,},
  title = {Calculating Rathole Geometry of
	
	Gravity Reclaim Stockpiles using Hoop Stress Theory and
	
	Modified Jenike Shear Tests},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {This paper briefly reviews a new hoop stress theory developed by the
	authors to update the long
	
	standing theories for funnel-flow derived originally by Jenike. The
	basis of the hoop stress theory
	
	involves the determination of the unconfined yield strength of the
	bulk solid in the third direction
	
	orthogonal to the 2-dimensional stress state upon which the original
	Jenike theory is based. Using the
	
	Jenike type direct shear tester, Flow Functions are generated by shearing
	test samples in the direction
	
	perpendicular to the plane governing the 2-dimensional consolidation.
	The unconfined yield strength
	
	governing the strength of the ratholes is determined from these Flow
	Functions. It is also shown that
	
	the unconfined yield strength in the third or circumferential direction
	may be approximated closely to
	
	the value corresponding to the mean stress determined using the conventional
	2-dimensional direct
	
	shear test. The application of the new theory, together with the procedures
	developed by TUNRA for
	
	determining draw-down and live capacities of gravity reclaim funnel-flow
	bins and stockpiles are
	
	described. Case study examples are presented to demonstrate the validity
	of this theory.},
  keywords = {Bulk Solids; Hoop Stress Theory; Funnel-Flow; Stockpiles; Draw-down;
	Live Capacity},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{Saccani2010,
  author = {C. Saccani, and M. Pellegrini,},
  title = {Plant design for atomized slip pneumatic conveying: environmental
	impact
	
	reduction in the ceramics industry avoiding product wear and fragmentation},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {Belt conveyors have proven themselves for continuous conveying of
	bulk material for decades under technical and
	
	economic points of view. For long belt conveyors and large amounts
	of bulk material, steel cord conveyor belts represent
	
	the state of art. An increase of the applied material flow can only
	be obtained by the development of high tensile steel cord
	
	conveyor belts. Particularly high nominal strengths have been applied
	in the case of conveying systems which have to
	
	overcome considerable lifting heights and high bulk material flow
	rates. Conveyor belts are first spliced to form a
	
	continuous belt on the conveying system site. This is mainly due to
	the fact that both the winding diameters as well as the
	
	weight of the conveyor belts represent limits considering of transportation
	and handling. Conveyor belts therefore consist of
	
	a number of belt sections which are bonded through vulcanization,
	these sections rarely exceeding 400 m. Each conveyor
	
	belt has at least one and generally a number of splices. The splices
	represent weak parts in the conveyor belt. The strength
	
	of the splices is lower than the nominal strength of the belt. There
	are currently two competing development targets which
	
	can be identified in steel cord conveyor belt splicing: on the one
	hand the strength of the splice of the steel conveyor belts
	
	should be increased, while possibly accepting an increase of the installation
	costs. On the other hand, particularly in opencast
	
	mining operations, the costs of installing the splices shall be reduced
	as much as possible, without negative effects to
	
	the strength of the splice. In any case the accurate design of conveyor
	belt splices is a requirement for the safe operation of
	
	the whole conveyor belt system. Due to these reasons, the Institute
	of Transport and Automation Technology (ITA) of the
	
	Leibniz University Hannover has extended its research in the field
	of dynamic tests for the determination of fatigue strength
	
	for conveyor belt splices of belts with a nominal strength higher
	than 7800 N/mm. Therefore the institute adapted its
	
	corresponding test facilities to the state of the art in the development
	of high tensile conveyor belts.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

