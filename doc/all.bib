@CONFERENCE{Bradley2010,
  author = {M.S.A. Bradley, and T. Deng, and R.J. Farnish,},
  title = {Prediction Pneumatic Conveyor Pipeline Pressure Drop for Design:
	
	Latest Advances in Characterisation and Design Techniques},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {Pneumatic conveying remains an important technique for moving material
	in many
	
	industries; prediction of pressure loss is a key design step enabling
	comparison of
	
	pipeline size options and determination of ancillary equipment (feeders,
	air mover) for
	
	specifying a successful system. Over the years it has been shown that
	the only
	
	reliable way to do this is by conveying a sample of the material to
	be handled, in a test
	
	pipeline, and using the data for design of the plant system. This
	is an expensive
	
	process, and various uncertainties in scale-up have been much discussed.
	
	This paper sets out to describe the development of the pneumatic conveying
	
	characterisation facility at The Wolfson Centre, and how it has been
	evolved over the
	
	last two decades so that the cost of material characterisation in
	a pipeline have been
	
	reduced, the problems associated with it have been mitigated and the
	accuracy of
	
	scale-up improved. Some case studies are given of data gathering,
	analysis and
	
	pipeline synthesis using the technique.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{McGlinchey2010,
  author = {Don McGlinchey and John Pugh and Liz Knight and Ayyapan Kumaraperumal
	and Ijidai Samaila and Taofeeq Ibn Mohammed,},
  title = {Operation of a Novel Industrial Scale Thermal Solids Mass Flowmeter.},
  booktitle = {BulkEurope 2010},
  year = {2010},
  publisher = {Bulk Europe 2010},
  abstract = {The in-line measurement of solids mass flow rate in an industrial
	scale pneumatic conveying
	
	pipeline by a thermal method has been investigated over a range of
	conveying conditions. A
	
	thermal method of determining solids mass flow rate should, in principle,
	be capable of achieving a
	
	reliable measurement regardless of; in homogeneities in solids’ distribution,
	irregularities in
	
	velocity profile and variations in particle size or shape. The instrument
	described in this paper
	
	operates by the injection of heat energy into the pipeline by way
	of a heated section and measuring
	
	the resultant change in solids’ temperature using infra-red sensors.
	The results from the thermal
	
	instrument are compared with measurements from a gain in weight system
	at the receiving vessel.},
  owner = {czoobs},
  timestamp = {2011.02.13}
}

