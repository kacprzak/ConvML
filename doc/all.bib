@CONFERENCE{BradleyAhmed2010,
  author = {MSA Bradley, and S. Ahmed, and T. Deng, and RJ Farnish,},
  title = {Pneumatic Conveyor Pipeline Pressure Drop in Lean Phase:
	
	Investigating the Role of Dynamic Particle Friction in Bends, on Bend
	Pressure
	
	Drop, using Strobe Particle Imaging Velocimetry (PIV) Techniques},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {The pressure losses caused by bends in a lean phase (dilute or suspension
	flow)
	
	pneumatic conveyor can often represent well over half the system energy
	
	consumption, so predicting bend pressure loss is very important in
	design. The
	
	only way to do this reliably at present is to convey a sample of the
	material
	
	through an instrumented pipeline to obtain data on the behaviour of
	the
	
	material, then use this for system design. Though a reliable and trustworthy
	
	approach, this is expensive and it would be useful to have a way to
	predict
	
	bend pressure drop from particle properties or a small scale test,
	especially
	
	when making a “first-cut” design for quotation purposes when it is
	hard to
	
	justify making significant investment into the outline design. Previous
	
	attempts to correlate pressure loss against particle/steel sliding
	friction in
	
	steady linear movement have not helped, so this work set out to characterise
	
	the dynamic friction between particle and wall as a single particle
	moves
	
	through a real bend.
	
	This report shows the success that has been achieved to date in capturing
	the
	
	friction data using basic strobe PIV techniques. Although some limitations
	
	have been encountered, sufficient data has been captured to make a
	useful
	
	comparison between different particles and against measured bend pressure
	
	losses using previous models. The approach seems to show some promise,
	and
	
	approaches for taking the work forward are discussed.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

