@CONFERENCE{Miccio2010,
  author = {F. Miccio, and N. Silvestri, and D. Barletta, and M. Poletto,},
  title = {Arch stability in consolidated samples of woody biomass},
  booktitle = {BulKEurope 2010},
  year = {2010},
  abstract = {In this paper a simple method for characterizing the arch stability
	of consolidated samples of woody biomass is presented.
	
	The apparatus used consists in a cylindrical bin provided with a circular
	orifice at the centre of its flat bottom. Biomass
	
	samples were consolidated in the bin by applying loads on the material
	bed surface while the bin orifice was closed by a
	
	plug. After the consolidation phase the loads were removed and the
	orifice was opened to assess if an arch or a rathole had
	
	formed. Critical values of consolidation loads for the formation of
	a stable arch were determined for different orifice sizes.
	
	These results obtained with two different samples of sawdust were
	compared in terms of material strength with those of
	
	flow functions obtained with a conventional Schulze shear tester.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{Hondel2010,
  author = {Gerard van den Hondel MSc, BSc,},
  title = {Aramids for low weight and low rolling resistance of conveyor belts},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {Two aramid products can substantially reduce the energy needed to
	drive conveyor belts, one
	
	by reducing the weight of the belt and the other by reducing hysteresis
	of the belt rubber.
	
	The heavier a conveyor belt is the more energy is needed to drive
	it. At the same strength,
	
	aramid fiber is 5 times lighter than steel and 3 times lighter than
	polyester. An aramid
	
	reinforced conveyor belt is much lighter than a steel belt so a reduction
	in energy
	
	consumption of around 10% can be achieved. Twaron aramid yarn can
	readily replace steel as
	
	belt reinforcement and references are available on long haul belts
	that are in successful
	
	operation since many years.
	
	Over half of a conveyor belt’s driving energy is lost in indentation
	rolling resistance of the
	
	belt. This is due to hysteresis when a rubber belt passes over its
	support rollers. Sulfron is a
	
	new Twaron-based rubber ingredient that lowers hysteresis. Data are
	available on reduced tan
	
	δ (loss factor of energy in hysteresis) in several applications and
	a reduction in belt driving
	
	power of 15% is expected. First tests on belts are in progress.
	
	Thus due to its low weight, Twaron aramid as belt reinforcement reduces
	weight and hence
	
	the energy to drive the belt. Sulfron reduces hysteresis in belt rubbers
	and has the potential to
	
	reduce indentation rolling resistance considerably.},
  journal = {Bulk Europe 2010},
  owner = {czoobs},
  timestamp = {2011.02.13}
}

@CONFERENCE{Mustoe2010,
  author = {Graham G. W. Mustoe, and Clint Hudson and Allen Reicks},
  title = {Simulation of Mixing and Segregation Phenomena in Industrial
	
	Material Flows with Discrete Element Methods},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {This paper presents the theoretical basis of a mixing index methodology
	that can be used to quantitatively
	
	predict the degree of mixing or segregation in a discrete element
	analysis of a system of particles with nonuniform
	
	properties. The non-uniformity in the particle system may be of a
	general nature. For example, the
	
	system of particles may have a non-uniform size distribution or different
	material properties such as
	
	density, friction, etc.
	
	The overall concept in the development of the mixing index methodology
	is to compute a quantitative
	
	measure of the difference between the evolutions of the states of
	the system of particles as predicted in a
	
	discrete element analysis with that of an ideally mixed system. A
	spatial grid is used to bin the particles
	
	into a system of grid cells. This data is then analyzed with a squared
	difference approach that determines a
	
	normalized mixing index value such that a perfectly mixed particle
	system has a mixing index value of 1,
	
	and a perfectly segregated particle system has a mixing index value
	of 0.
	
	The application of the mixing index methodology is illustrated by
	analyzing the particle system position
	
	data generated from a three-dimensional discrete element analysis.
	A case study of industrial significance
	
	with complex geometries have been analyzed. Four datasets of particle
	system data were generated from
	
	three-dimensional discrete element analyses of a rotating cylindrical
	kiln with baffles and without baffles.
	
	These analyses show that this mixing index methodology can be readily
	applied to quantitatively determine
	
	mixing or segregation rates for a wide range of industrial mixing
	systems used in bulk solids handling.},
  owner = {czoobs},
  timestamp = {2011.02.20}
}

@CONFERENCE{Gruening2010,
  author = {Gruening T., and Kunze G., and Katterfeld A.,},
  title = {Simulating the working process of construction machines},
  booktitle = {BulkEurope 2010},
  year = {2010},
  abstract = {The working process of mining and construction machines is a complex
	interaction of the machine itself and the bulk material. Especially
	when optimising the design or operation of an excavator for instance,
	a deep understanding of the ongoing processes is essential. Computer
	simulations based on the Discrete Element Method (DEM) offer a possibility
	to comprehend the bulk material behaviour. In contrast to other DEM
	simulations, the bulk material has a strong influence on the machine
	dynamics during the excavation process. Hence, realistic simulation
	results are only possible, if the DEM simulation can be combined
	with a simulation of the whole machine, including multi body systems
	and hydraulics. This paper presents an introduction to the simulation
	techniques and the coupling principle used for an examplary simulation
	of a bucket excavator and a wheel loader.},
  owner = {czoobs},
  timestamp = {2011.02.13}
}

@CONFERENCE{Heinrici2010,
